{% extends "base-layout.twig" %}

{% block title %}{{ parent() }}{{ title }}{% endblock %}
{% block head_css %}
    {{ parent() }}
    <style type="text/css">
        .logined-user {
            padding-right: 0.5em;
        }
        .page-header {
            margin: 10px 0 16px 0;
        }
        .sidebar ul li a.current-active {
            background-color: #d2d2d2;
        }
        .weui-tabbar__item.current{
            color: #4ed8bd;
        }
        .weui-tabbar__item.current p{
            color:#4ed8bd ;
        }
    </style>
{% endblock %}
{% block head_js %}
    {{ parent() }}
    <script type="text/javascript">
    if(!window.GLOBAL) {
        window.GLOBAL = {};
    }
    if(!GLOBAL.CHANGEPWD_URL) {
        GLOBAL.UID = "{{ 'test' }}";
    }
    </script>
{% endblock %}
{% block dialog %}
    {{ parent() }}
{% endblock %}
{% block header %}
    {# {% include 'admin/_common/modal-password.twig' ignore missing %} #}
    {{ parent() }}
{% endblock %}
{% block content %}
    {{ parent() }}

{% endblock %}
{% block footer %}
    {{ parent() }}
{% endblock %}
{% block foot_js %}
    {% set main_prefix = 'login-' %}
    {{ parent() }}
{% endblock %}