<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    {# Set render engine for 360 browser #}
    <meta name="renderer" content="webkit" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1,user-scalable=no" />
    {# No Baidu Siteapp #}
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="shortcut icon" href="{{ base_url() }}/favicon.ico">
    {# Add to homescreen for Chrome on Android #}
    <meta name="mobile-web-app-capable" content="yes" />
    <link rel="icon" sizes="192x192" type="image/png" href="{{ base_url() }}/favicon.png">
    {# Add to homescreen for Safari on iOS #}
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="apple-mobile-web-app-title" content="Amaze UI" />
    <link rel="apple-touch-icon-precomposed" href="{{ base_url() }}/favicon.png">
    {# Tile icon for Win8 (144x144 + tile color) #}
    <meta name="msapplication-TileImage" content="{{ base_url() }}/favicon.png" />
    <meta name="msapplication-TileColor" content="#0e90d2" />
    {# seo #}
    <meta name="keywords" content="keywords" />
    <meta name="description" content="description" />
    <meta name="author" content="cloudkz.cn[gsw945]" />
    <link rel="stylesheet" href="{{ base_url() }}/public/lib/weui/weui.css">
    <link rel="stylesheet" type="text/css" href="{{ base_url() }}/public/assets/home/css/common.css">
    <link rel="stylesheet" type="text/css" href="{{ base_url() }}/public/lib/wechatIcon/iconfont.css">
    <title>{% block title %}数字资源管理平台{% endblock %}</title>
    {% block head_css %}

    {% endblock %}
    {% block head_js %}

    {% endblock %}
</head>
<body>
    {% block dialog %}
        {% include "_dialog/alert.twig" %}
        {% include "_dialog/confirm.twig" %}
        {% include "_dialog/loading.twig" %}
        {% include "_dialog/prompt.twig" %}
    {% endblock %}
    {% block header %}{% endblock %}
    {% block content %}{% endblock %}
    {% block footer %}{% endblock %}
    {% block foot_js %}
    <script type="text/javascript">
        // requirejs main模块加载超时解决
        var requirejs = {
            waitSeconds: 0
        };
        if(!window.GLOBAL) {
            window.GLOBAL = {};
        }
        if(!GLOBAL.SITE_BASE_URL) {
            GLOBAL.SITE_BASE_URL = "{{ site_base_url }}";
        }

    </script>
    <script type="text/javascript" src="{{ base_url() }}/public/lib/requirejs/require-2.3.2.min.js" data-main="{{ base_url() }}/public/js/{{ main_prefix }}main.js"></script>
    {% endblock %}
</body>
</html>
